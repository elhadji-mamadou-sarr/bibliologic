{% extends 'base.html.twig' %}

{% block title %}Emprunteur{% endblock %}

{% block body %}
    <h1>Emprunteur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ emprunteur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ emprunteur.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ emprunteur.prenom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ emprunteur.adresse }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ emprunteur.telephone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ emprunteur.email }}</td>
            </tr>
            <tr>
                <th>Date_inscription</th>
                <td>{{ emprunteur.dateInscription ? emprunteur.dateInscription|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_emprunteur_index') }}">back to list</a>

    <a href="{{ path('app_emprunteur_edit', {'id': emprunteur.id}) }}">edit</a>

    {{ include('emprunteur/_delete_form.html.twig') }}
{% endblock %}
